<div class="brick {{type}}" id="{{id}}">
	{{! dynamic partial rendering }}
	{{#dynamicPartial}}
	    _{{type}}
	{{/dynamicPartial}}

	{{! always render overlay }}
	<div class="overlay">
		<a class="remove" href="#" data-id="{{id}}" data-key="{{key}}">X</a>
		<a target="_blank" href="{{url}}">
			<div style="height:100%;width:100%;">
				<div class="inner blogName">{{user}}</div>	
			</div>
		</a>
	</div>
</div>
